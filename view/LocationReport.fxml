<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LocationReportController">
   <Label layoutX="11.0" prefHeight="69.0" prefWidth="273.0" text="Location Report">
      <font>
         <Font size="36.0" />
      </font>
   </Label>
   <ComboBox fx:id="CountryBox" layoutX="11.0" layoutY="82.0" onAction="#ChooseCountry" prefHeight="27.0" prefWidth="150.0" promptText="Country Filter" />
   <ComboBox fx:id="DivisionBox" layoutX="171.0" layoutY="82.0" onAction="#ChooseDivision" prefHeight="27.0" prefWidth="150.0" promptText="Division Filter" />
   <Button fx:id="MainMenu" layoutX="682.0" layoutY="82.0" mnemonicParsing="false" onAction="#returnMainMenu" prefHeight="12.0" prefWidth="114.0" text="Main Menu">
      <font>
         <Font size="13.0" />
      </font>
   </Button>
   <TableView fx:id="CustomerTable" layoutY="123.0" prefHeight="275.0" prefWidth="810.0">
      <columns>
         <TableColumn fx:id="CustomerIDColumn" prefWidth="84.0" text="Customer ID" />
         <TableColumn fx:id="NameColumn" prefWidth="101.0" text="Name" />
         <TableColumn fx:id="AddressColumn" prefWidth="149.0" text="Address" />
         <TableColumn fx:id="PostalColumn" prefWidth="91.0" text="Postal" />
         <TableColumn fx:id="PhoneColumn" prefWidth="106.0" text="Phone" />
         <TableColumn fx:id="DivisionColumn" prefWidth="100.0" text="Division" />
         <TableColumn fx:id="CountryColumn" prefWidth="96.0" text="Country" />
         <TableColumn fx:id="DivisionIDColumn" prefWidth="77.0" text="Division ID" />
      </columns>
   </TableView>


</AnchorPane>
